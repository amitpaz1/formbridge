  "humanRequired": [
    "principals[*].ssn — Sensitive, must be entered directly",
    "principals[*].dateOfBirth — PII",
    "principals[*].homeAddress — Private addresses",
    "principals[*].personalNetWorth — Self-reported",
    "principals[*].consentToCredit — Must personally consent",
    "applicant.ein — Sensitive business info",
    "financials.bankContactName/Phone — Need to look up",
    "financials.outstandingLoans — Need to check records",
    "guarantor (all details) — Guarantor must fill their own section",
    "references.tradeReferences — Need to gather contact info",
    "documents (all) — Must upload actual PDFs: tax returns, financials, bank statements, etc.",
    "applicationTerms.offeredRent — Negotiation decision",
    "applicationTerms.applicantSignature — Must sign personally"
  ]
}
