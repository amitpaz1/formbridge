{
  "id": "commercial-lease-application",
  "version": "1.0.0",
  "name": "Commercial Lease Application",
  "description": "Real estate agent AI pre-fills property details and business info from a viewing tour. Applicant uploads financial documents, provides guarantor info, and signs the application.",
  "schema": {
    "type": "object",
    "properties": {
      "property": {
        "type": "object",
        "title": "Property Details",
        "properties": {
          "listingId": { "type": "string", "title": "Listing ID" },
          "address": {
            "type": "object",
            "properties": {
              "street": { "type": "string" },
              "unit": { "type": "string" },
              "city": { "type": "string" },
              "state": { "type": "string" },
              "zipCode": { "type": "string" }
            },
            "required": ["street", "city", "state", "zipCode"]
          },
          "squareFootage": { "type": "integer", "title": "Square Footage", "minimum": 100 },
          "leaseType": {
            "type": "string",
            "title": "Lease Type",
            "enum": ["NNN", "gross", "modified-gross", "percentage"]
          },
          "askingRent": { "type": "number", "title": "Asking Monthly Rent ($)" },
          "leaseTerm": {
            "type": "object",
            "title": "Proposed Lease Term",
            "properties": {
              "startDate": { "type": "string", "format": "date" },
              "durationMonths": { "type": "integer", "minimum": 6, "maximum": 180 },
              "renewalOptions": { "type": "integer", "title": "Number of Renewal Options", "default": 0 },
              "renewalTermMonths": { "type": "integer", "title": "Each Renewal Term (months)" },
              "annualEscalation": {
                "type": "number",
                "title": "Annual Rent Escalation (%)",
                "minimum": 0,
                "maximum": 10
              }
            },
            "required": ["startDate", "durationMonths"]
          },
          "intendedUse": {
            "type": "string",
            "title": "Intended Use of Premises",
            "minLength": 10,
            "maxLength": 500
          },
          "zoningConfirmed": { "type": "boolean", "title": "Zoning confirmed for intended use?" },
          "parkingSpaces": { "type": "integer", "title": "Parking Spaces Included", "minimum": 0 },
          "tenantImprovements": {
            "type": "object",
            "title": "Tenant Improvement (TI) Allowance",
            "properties": {
              "requested": { "type": "boolean" },
              "estimatedCost": { "type": "number", "title": "Estimated TI Cost ($)" },
              "description": { "type": "string", "title": "Description of Improvements" },
              "landlordContribution": { "type": "number", "title": "Requested Landlord Contribution ($)" }
            }
          }
        },
        "required": ["listingId", "address", "squareFootage", "leaseType", "askingRent", "leaseTerm", "intendedUse"]
      },
      "applicant": {
        "type": "object",
        "title": "Applicant / Business Entity",
        "properties": {
          "businessName": { "type": "string", "title": "Legal Business Name" },
          "dba": { "type": "string", "title": "DBA (if different)" },
          "entityType": {
            "type": "string",
            "title": "Entity Type",
            "enum": ["sole-proprietorship", "partnership", "llc", "corporation", "non-profit"]
          },
          "stateOfFormation": { "type": "string", "title": "State of Formation" },
          "ein": {
            "type": "string",
            "title": "Federal EIN",
            "pattern": "^[0-9]{2}-[0-9]{7}$"
          },
          "yearEstablished": { "type": "integer", "title": "Year Established" },
          "numberOfEmployees": { "type": "integer", "title": "Number of Employees" },
          "website": { "type": "string", "title": "Website", "format": "uri" },
          "naicsCode": { "type": "string", "title": "NAICS Code" },
          "businessDescription": {
            "type": "string",
            "title": "Business Description",
            "minLength": 20,
            "maxLength": 1000
          },
          "currentAddress": {
            "type": "object",
            "title": "Current Business Address",
            "properties": {
              "street": { "type": "string" },
              "city": { "type": "string" },
              "state": { "type": "string" },
              "zipCode": { "type": "string" }
            },
            "required": ["street", "city", "state", "zipCode"]
          },
          "reasonForMoving": {
            "type": "string",
            "title": "Reason for Relocating",
            "enum": ["expansion", "downsizing", "new-business", "lease-expiring", "better-location", "cost-reduction", "other"]
          }
        },
        "required": ["businessName", "entityType", "ein", "businessDescription"]
      },
      "principals": {
        "type": "array",
        "title": "Principals / Owners (20%+ ownership)",
        "items": {
          "type": "object",
          "properties": {
            "fullName": { "type": "string", "title": "Full Legal Name" },
            "title": { "type": "string", "title": "Title" },
            "ownershipPercentage": { "type": "number", "title": "Ownership %", "minimum": 20, "maximum": 100 },
            "ssn": {
              "type": "string",
              "title": "SSN",
              "description": "Required for credit check — human-only"
            },
            "dateOfBirth": { "type": "string", "format": "date" },
            "homeAddress": {
              "type": "object",
              "properties": {
                "street": { "type": "string" },
                "city": { "type": "string" },
                "state": { "type": "string" },
                "zipCode": { "type": "string" }
              },
              "required": ["street", "city", "state", "zipCode"]
            },
            "phone": { "type": "string" },
            "email": { "type": "string", "format": "email" },
            "personalNetWorth": { "type": "number", "title": "Approximate Personal Net Worth ($)" },
            "bankruptcyHistory": { "type": "boolean", "title": "Any bankruptcy filings in last 7 years?", "default": false },
            "litigationHistory": { "type": "boolean", "title": "Any pending litigation?", "default": false },
            "consentToCredit": {
              "type": "boolean",
              "title": "I authorize a credit check for this application",
              "const": true
            }
          },
          "required": ["fullName", "ownershipPercentage", "ssn", "dateOfBirth", "homeAddress", "consentToCredit"]
        },
        "minItems": 1
      },
      "financials": {
        "type": "object",
        "title": "Financial Information",
        "properties": {
          "annualRevenue": { "type": "number", "title": "Annual Revenue (most recent year)" },
          "projectedRevenue": { "type": "number", "title": "Projected Annual Revenue (next year)" },
          "monthlyOperatingExpenses": { "type": "number", "title": "Monthly Operating Expenses" },
          "bankName": { "type": "string", "title": "Primary Bank" },
          "bankContactName": { "type": "string", "title": "Bank Contact" },
          "bankContactPhone": { "type": "string", "title": "Bank Phone" },
          "accountingMethod": {
            "type": "string",
            "title": "Accounting Method",
            "enum": ["cash", "accrual"]
          },
          "outstandingLoans": {
            "type": "array",
            "title": "Outstanding Loans / Debts",
            "items": {
              "type": "object",
              "properties": {
                "lender": { "type": "string" },
                "originalAmount": { "type": "number" },
                "remainingBalance": { "type": "number" },
                "monthlyPayment": { "type": "number" },
                "maturityDate": { "type": "string", "format": "date" }
              }
            }
          }
        },
        "required": ["annualRevenue", "bankName"]
      },
      "guarantor": {
        "type": "object",
        "title": "Personal Guarantor",
        "description": "Required if business is less than 3 years old or revenue < 3x annual rent",
        "properties": {
          "required": { "type": "boolean", "title": "Personal guarantee required?" },
          "fullName": { "type": "string" },
          "relationship": { "type": "string", "enum": ["owner", "partner", "investor", "family-member", "other"] },
          "ssn": { "type": "string", "title": "SSN" },
          "dateOfBirth": { "type": "string", "format": "date" },
          "homeAddress": {
            "type": "object",
            "properties": {
              "street": { "type": "string" },
              "city": { "type": "string" },
              "state": { "type": "string" },
              "zipCode": { "type": "string" }
            }
          },
          "annualIncome": { "type": "number" },
          "netWorth": { "type": "number" },
          "consentToCredit": { "type": "boolean", "const": true },
          "guaranteeSignature": { "type": "string", "title": "Guarantor Signature" },
          "guaranteeDate": { "type": "string", "format": "date" }
        }
      },
      "references": {
        "type": "object",
        "title": "References",
        "properties": {
          "currentLandlord": {
            "type": "object",
            "title": "Current/Previous Landlord",
            "properties": {
              "name": { "type": "string" },
              "phone": { "type": "string" },
              "propertyAddress": { "type": "string" },
              "leaseDuration": { "type": "string" },
              "monthlyRent": { "type": "number" }
            }
          },
          "tradeReferences": {
            "type": "array",
            "title": "Trade References",
            "items": {
              "type": "object",
              "properties": {
                "companyName": { "type": "string" },
                "contactName": { "type": "string" },
                "phone": { "type": "string" },
                "relationship": { "type": "string" },
                "yearsKnown": { "type": "integer" }
              }
            },
            "minItems": 2,
            "maxItems": 5
          },
          "accountantOrAttorney": {
            "type": "object",
            "title": "CPA / Attorney Reference",
            "properties": {
              "name": { "type": "string" },
              "firm": { "type": "string" },
              "phone": { "type": "string" },
              "email": { "type": "string", "format": "email" }
            }
          }
        },
        "required": ["tradeReferences"]
      },
      "documents": {
        "type": "object",
        "title": "Required Documents (upload)",
        "properties": {
          "taxReturns": {
            "type": "array",
            "title": "Business Tax Returns (last 3 years)",
            "items": { "type": "string", "format": "uri" },
            "minItems": 1,
            "maxItems": 3
          },
          "financialStatements": {
            "type": "array",
            "title": "Financial Statements (P&L, Balance Sheet)",
            "items": { "type": "string", "format": "uri" },
            "minItems": 1
          },
          "bankStatements": {
            "type": "array",
            "title": "Bank Statements (last 6 months)",
            "items": { "type": "string", "format": "uri" },
            "minItems": 3,
            "maxItems": 6
          },
          "articlesOfOrganization": {
            "type": "string",
            "title": "Articles of Incorporation / Organization",
            "format": "uri"
          },
          "businessLicense": {
            "type": "string",
            "title": "Business License",
            "format": "uri"
          },
          "insuranceCertificate": {
            "type": "string",
            "title": "Certificate of Insurance (or proof of insurability)",
            "format": "uri"
          },
          "personalFinancialStatement": {
            "type": "array",
            "title": "Personal Financial Statement (each principal)",
            "items": { "type": "string", "format": "uri" }
          }
        },
        "required": ["taxReturns", "financialStatements", "bankStatements"]
      },
      "applicationTerms": {
        "type": "object",
        "title": "Application Terms & Signature",
        "properties": {
          "applicationFee": {
            "type": "number",
            "title": "Non-refundable Application Fee ($)",
            "default": 500
          },
          "acknowledgeTerms": {
            "type": "boolean",
            "title": "I acknowledge all information is true and complete. I authorize verification of all statements.",
            "const": true
          },
          "acknowledgeCredit": {
            "type": "boolean",
            "title": "I authorize credit and background checks for all principals listed above.",
            "const": true
          },
          "applicantSignature": { "type": "string", "title": "Applicant Signature" },
          "signatureDate": { "type": "string", "format": "date" },
          "offeredRent": {
            "type": "number",
            "title": "Offered Monthly Rent ($)",
            "description": "Your proposed rent (may differ from asking price)"
          },
          "proposedSecurityDeposit": {
            "type": "number",
            "title": "Proposed Security Deposit ($)"
          },
          "additionalTerms": {
            "type": "string",
            "title": "Additional Terms or Conditions",
            "description": "Any special requests or negotiation points",
            "maxLength": 2000
          }
        },
        "required": ["acknowledgeTerms", "acknowledgeCredit", "applicantSignature", "signatureDate"]
      }
    },
    "required": ["property", "applicant", "principals", "financials", "references", "documents", "applicationTerms"]
  },
